{
    "name": "debug",
    "device": "cpu",

    "arch": {
        "type": "CustomResNet50",
        "args": {
            "num_classes": 3
        }
    },
    "preprocessing": {
        "dir_path": "data",
        "transform_x": "resize_rgb",
        "transform_y": false
    },
    "dataset": {
        "type": "FromJSON",
        "args": {
            "images_labels_path": "data/split_data/train/images_labels.json"
        }
    },
    "dataloader": {
        "type": "DataloaderFromDataset",
        "args":{
            "batch_size": 16,
            "shuffle": true,
            "validation_split": 0.2,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "mse_loss",
    "metrics": [
        "IoU"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "monitor": "min val_loss",
        "early_stop": 10,
        "save_period": 1
    }
}
